{
	"$schema": "https://developer.microsoft.com/json-schemas/teams/v1.17/MicrosoftTeams.schema.json",
	"manifestVersion": "1.17",
	"version": "1.4.0",
	"id": "3b60a7e6-440e-4777-a769-c4c4c3f2e244",
	"developer": {
		"name": "Rylogic",
		"websiteUrl": "https://www.rylogic.co.nz",
		"privacyUrl": "https://www.rylogic.co.nz/privacy",
		"termsOfUseUrl": "https://www.rylogic.co.nz/terms"
	},
	"name": {
		"short": "Whiteboard Live",
		"full": "Whiteboard Live - Diagramming Tool"
	},
